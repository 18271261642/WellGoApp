            <enum name="RIGHT" value="2" />
        </attr>
        <attr name="points_container_background" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="CompoundGroup">
        <!-- The id of the child radio button that should be checked by default
             within this radio group. -->
        <attr name="checkedPosition" format="integer"/>
        <attr name="checkedType" format="enum">
            <enum name="single" value="0" />
            <enum name="multi" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="StatusBarHeightView">

        <attr name="use_type" format="integer">
            <enum name="use_height" value="0" />
            <enum name="use_padding_top" value="1" />
